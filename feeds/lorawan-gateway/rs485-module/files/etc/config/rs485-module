config mqtt 'mqtt'
        option enabled '0'
        option transport 'tcp'
        option host ''
        option port '1883'
        option client_id ''
        option keepalive '30'
        option username ''
        option password ''
        option auth_mode 'none'
        option uplink_topic 'rs485/uplink'
        option downlink_topic 'rs485/downlink'
        option clean_session '1'
        option qos '0'
        option reconnect_delay '30'
        option retain_status '0'

config serial 'serial'
        option enabled '1'
        option device 'ttyAMA2'
        option baudrate '9600'
        option databit '8'
        option stopbit '1'
        option checkbit 'none'
        option flowcontrol 'none'
        option timeout '1000'

config log 'ui'
        option auto_refresh '1'
        option buffer_limit '2000'
        list watch_keywords 'join'
        list watch_keywords 'error'

config protocol 'protocol'
        option enabled '0'
        option type 'modbus-rtu'
        option device_address '1'
        option function_code '03'
        option register_address '40001'
        option data_length '10'
        option poll_interval '1000'
        option enable_crc '1'
        option write_value '0'
